<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label "Favorite Genre" %>
    <%= f.text_field :fav_genre %>
    <%= f.label :neighborhood %>
    <%= f.text_field :neighborhood %>
    <%= f.submit %>
<%end%>