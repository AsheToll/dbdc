<h1>Write a Review</h1>

<%= form_for @review do |f| %>
    <%= f.hidden_field :user_id, value: session[:user_id] %>
    <%= f.collection_select(:dive_bar_id, DiveBar.all, :id, :name) %>
    <%= f.label :rating %>
    <%= f.select :rating, (1..10) %>
    <%= f.label :review %>
    <%= f.text_field :review %>
    <%= f.submit %>
    <%end%>