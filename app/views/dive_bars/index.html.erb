<h1>Dive Bars</h1>

<%= form_tag(dive_bars_path, method: "get") do %>
    <%= label_tag(:q, "Search By Neighborhood") %>
    <%= text_field_tag(:q, params[:q]) %>
    <%= submit_tag("Search") %>
<% end %>

<div class = "grid-container">
    <% @dive_bars.each do |dive_bar| %>
        <%= render partial: "bar_card", locals: {dive_bar: dive_bar}%>
    <% end %>
</div>